{
	"name": "urn-bld",
	"version": "0.0.1",
	"description": "Uranio build module for testing urn-cli",
	"main": "dist/src/index.js",
	"types": "dist/src/index.d.ts",
	"scripts": {
		"build": "tsc -b",
		"dev": "./node_modules/.bin/tsc-watch --onSuccess \"node -r source-map-support/register -r module-alias/register ./dist/src/index.js\""
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@bitbucket.org/nbl7/urn-bld.git"
	},
	"keywords": [
		"uranio",
		"urn"
	],
	"author": "Andrea Reni <mail@nbl7.net>",
	"license": "ISC",
	"homepage": "https://bitbucket.org/nbl7/urn-bld#readme",
	"_moduleAliases": {
		"uranio": "./dist/.uranio/lib/",
		"uranio-books": "./dist/.uranio/server/books.js",
		"uranio-client": "./dist/.uranio/lib/client",
		"uranio-core": "./dist/.uranio/lib/core/"
	},
	"dependencies": {
		"mongoose": "^5.11.15",
		"urn-lib": "git+ssh://git@bitbucket.org/nbl7/urn-lib.git",
		"urn-trx-dep": "ssh://git@bitbucket.org/nbl7/urn-trx-dep",
		"urn-web-dep": "ssh://git@bitbucket.org/nbl7/urn-api-dep"
	},
	"devDependencies": {
		"@nuxt/types": "^2.15.7",
		"@nuxt/typescript-build": "^2.1.0",
		"esbuild": "^0.12.16",
		"esbuild-plugin-alias": "^0.1.2",
		"pug": "^3.0.2",
		"pug-plain-loader": "^1.1.0",
		"tsc-watch": "^4.2.3",
		"ttypescript": "^1.5.12",
		"typescript": "^4.3.2",
		"typescript-transform-paths": "^3.1.0",
		"urn-trx-dep-dev": "ssh://git@bitbucket.org/nbl7/urn-trx-dep-dev",
		"urn-web-dep-dev": "ssh://git@bitbucket.org/nbl7/urn-api-dep-dev",
		"vue-template-compiler": "^2.6.14"
	}
}
